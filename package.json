{
  "name": "nti-web-commons",
  "version": "0.0.179",
  "author": "nextthought",
  "description": "ReactJS Common Components",
  "license": "UNLICENSED",
  "main": "./lib/index.js",
  "style": "./lib/index.css",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.3.0"
  },
  "scripts": {
    "test": "make test",
    "start": "make check; webpack-dev-server -d --config ./webpack.config.test.js  --host 0.0.0.0 --port 8000 --quiet --watch --progress --inline --info false --content-base ./test/app/ || true",
    "prepublish": "not-in-install && make || in-install",
    "bump": "npm version",
    "preversion": "make test",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextThought/nti.web.component.commons.git"
  },
  "keywords": [
    "react",
    "editor",
    "modeled",
    "modeled body",
    "content",
    "component"
  ],
  "dependencies": {
    "classnames": "^2.0.0",
    "eases": "^1.0.8",
    "filesize": "^3.3.0",
    "invariant": "^2.2.1",
    "isempty": "0.0.1",
    "jstimezonedetect": "^1.0.6",
    "mime-types": "^2.1.11",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.4",
    "nti-commons": "^2.4.0",
    "nti-lib-dom": "^1.5.2",
    "nti-lib-interfaces": "^1.11.0",
    "nti-lib-locale": "^1.0.2",
    "nti-lib-ntiids": "^1.0.1",
    "nti-util-ios-version": "^1.0.0",
    "nti-util-logger": "^1.0.0",
    "nti-web-client": "^1.1.0",
    "react-transition-group": "^1.1.0",
    "react-day-picker": "^2.1.0",
    "react-router-component": "^0.36.0",
    "react-sticky": "^5.0.5",
    "zpad": "^0.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.1.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.1.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-react": "^6.1.0",
    "babel-preset-stage-1": "^6.1.0",
    "baggage-loader": "^0.3.0",
    "css-loader": "0.x.x",
    "enzyme": "^2.3.0",
    "eslint-config-nti-codestyle-js": "^1.1.0",
    "eslint-config-nti-codestyle-jsx": "^1.0.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "0.x.x",
    "in-publish": "^2.0.0",
    "jest": "^19.0.2",
    "jest-junit": "^1.4.0",
    "node-sass": "^4.5.0",
    "normalize.css": "^5.0.0",
    "nti-style-common": "^1.1.11",
    "postcss-loader": "^1.3.0",
    "raw-loader": "0.x.x",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "resolve-url-loader": "^2.0.0",
    "sass-loader": "^6.0.0",
    "sinon": "^2.0.0",
    "style-loader": "0.x.x",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.4.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*.spec.js"
    ],
    "coverageDirectory": "reports/coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura"
    ],
    "testResultsProcessor": "./node_modules/jest-junit",
    "roots": [
      "<rootDir>/src/"
    ],
    "testMatch": [
      "**/__test__/**/*.(spec).js?(x)"
    ]
  },
  "jest-junit": {
    "output": "reports/test-results/index.xml"
  }
}